{
    "name": "Architectural Block Format",
    "description": "Mapping format for architectural blocks from Excel catalog",
    "version": "1.5",
    "table_settings": {
        "format_type": "xlsx",
        "sheet_name": "Blocks",
        "start_row": 3,
        "tutorial_row": 2
    },
    "column_mappings": {
        "ID": {
            "node_type": "SpecialFindUnit",
            "is_id": true,
            "description": "NUMERO INVENTARIO/CODICE BLOCCO: Indicare numero di inventario Parco archeologico (se presente). Se non inventariato, indicare qui il codice blocco assegnato su base tipologica (se ci sono entrambi, dare priorità all'inventario)"
        },
        "DEFINITION": {
            "node_type": "SpecialFindUnit",
            "is_description": true,
            "display_name": "Definition",
            "description": "DEFINIZIONE: Indicare definizione del blocco architettonico (es. capitello corinzio, base di colonna, ecc.)"
        },
        "MORPHOLOGY": {
            "node_type": "PropertyNode",
            "property_name": "Morphology",
            "display_name": "Morphology",
            "description": "MORFOLOGIA: scegliere la morfologia specifica del pezzo (es. capitello corinzio, dorico, ionico, ecc.)"
        },
        "MATERIAL": {
            "node_type": "PropertyNode",
            "property_name": "Material",
            "display_name": "Material",
            "description": "MATERIALE: Indicare il materiale del blocco"
        },
        "HEIGHT": {
            "node_type": "PropertyNode",
            "property_name": "Height",
            "display_name": "Height",
            "description": "ALTEZZA: indicare in cm"
        },
        "LENGTH": {
            "node_type": "PropertyNode", 
            "property_name": "Length",
            "display_name": "Length",
            "description": "LUNGHEZZA: indicare in cm"
        },
        "WIDTH": {
            "node_type": "PropertyNode",
            "property_name": "Width", 
            "display_name": "Width",
            "description": "PROFONDITÀ: indicare in cm"
        },
        "UPPER_FACE_LENGTH": {
            "node_type": "PropertyNode",
            "property_name": "UpperFaceLength",
            "display_name": "Upper Face Length",
            "description": "LUNGHEZZA PIANO DI ATTESA: lunghezza del piano di attesa in cm"
        },
        "UPPER_FACE_WIDTH": {
            "node_type": "PropertyNode",
            "property_name": "UpperFaceWidth",
            "display_name": "Upper Face Width",
            "description": "PROFONDITÀ PIANO DI ATTESA: profondità del piano di attesa in cm"
        },
        "BOTTOM_FACE_LENGTH": {
            "node_type": "PropertyNode",
            "property_name": "BottomFaceLength",
            "display_name": "Bottom Face Length", 
            "description": "LARGHEZZA PIANO DI POSA: larghezza al piano di posa in cm"
        },
        "BOTTOM_FACE_WIDTH": {
            "node_type": "PropertyNode",
            "property_name": "BottomFaceWidth",
            "display_name": "Bottom Face Width",
            "description": "PROFONDITÀ PIANO DI POSA: profondità al piano di posa in cm"
        },
        "UPPER_DIAMETER": {
            "node_type": "PropertyNode",
            "property_name": "UpperDiameter",
            "display_name": "Upper Diameter",
            "description": "DIAMETRO SUPERIORE: diametro superiore in cm (nei fusti corrisponde al diametro del piano di attesa)"
        },
        "BOTTOM_DIAMETER": {
            "node_type": "PropertyNode",
            "property_name": "BottomDiameter", 
            "display_name": "Bottom Diameter",
            "description": "DIAMETRO INFERIORE: diametro inferiore in cm (nei fusti corrisponde al diametro del piano di posa)"
        },
        "UPPER_DIAMETER_AT_APOPHYGE": {
            "node_type": "PropertyNode",
            "property_name": "UpperDiameterApophyge",
            "display_name": "Upper Diameter at Apophyge",
            "description": "DIAMETRO SUPERIORE ALL'APOPHYGE: diametro superiore all'apophyge in cm"
        },
        "BOTTOM_DIAMETER_AT_APOPHYGE": {
            "node_type": "PropertyNode", 
            "property_name": "BottomDiameterApophyge",
            "display_name": "Bottom Diameter at Apophyge",
            "description": "DIAMETRO INFERIORE ALL'APOPHYGE: diametro inferiore all'apophyge in cm"
        },
        "BIBLIOGRAPHY": {
            "node_type": "PropertyNode",
            "property_name": "Bibliography",
            "display_name": "Bibliography",
            "description": "BIBLIOGRAFIA: bibliografia del pezzo se già edito"
        },
        "2D-3D_DOCUMENTS": {
            "node_type": "PropertyNode",
            "property_name": "Documentation",
            "display_name": "Documentation",
            "description": "DOCUMENTAZIONE 2D-3D: URL di una cartella contenente tutta la documentazione relativa al blocco (disegni, foto, ecc. d'archivio da varie fonti, modello 3D, ecc.)"
        },
        "AUTHOR": {
            "node_type": "PropertyNode",
            "property_name": "Author",
            "display_name": "Field Compilation Author",
            "description": "RESPONSABILE COMPILAZIONE SUL CAMPO: indicare l'autore della scheda"
        },
        "NOTES": {
            "node_type": "PropertyNode",
            "property_name": "Notes",
            "display_name": "Notes",
            "description": "OSSERVAZIONI: testo libero con annotazioni utili"
        },
        "SECONDARY_MODIFICATIONS": {
            "node_type": "PropertyNode",
            "property_name": "SecondaryModifications",
            "display_name": "Secondary Modifications",
            "description": "RESTAURI, RILAVORAZIONI, RIUSI: indicare le modifiche secondarie (lavorazione superficiale, reimpiego, riparatura, inserti per prechellatura)"
        }
    },
    "relations": [
